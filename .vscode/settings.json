{
    "rust-analyzer.showUnlinkedFileNotification": false,
    // Doesn't show error for compile_error! macro
    "rust-analyzer.check.overrideCommand": [
        "cargo",
        "clippy",
        "--all-features",
        "--message-format=json",
        "--all-targets",
        "--workspace",
        "--all",
    ],
    "rust-analyzer.cargo.features": "all",
}